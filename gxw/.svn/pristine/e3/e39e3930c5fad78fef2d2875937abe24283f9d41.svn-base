  border:1px solid #54a5d5;
}
.Float .name{
  position:absolute;
  z-index:6200103;
  left:72px;
  top:5px;
  width:160px;
  height:25px;
  color:#1670eb;
}
.Float .txt{
  position:absolute;
  z-index:6200103;
  left:72px;
  top:30px;
  width:160px;
  height:60px;
  color:#808080;
}
.Float .masterbtns{
  position:absolute;
  z-index:6200103;
  left:0px;
