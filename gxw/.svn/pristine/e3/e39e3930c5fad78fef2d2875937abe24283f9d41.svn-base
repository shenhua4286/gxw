/*好友开始*/
#friendbody{
  background:#ffffff;
  height:20px;
  line-height:20px;
}
.friendlist{
}
.friendlist img{
  width:16px;
  height:16px;
}
.friendlist th{
  height:20px;
  font-weight:normal;
  text-align:left;
  padding-left:3px;
  background:url(friendTh.gif);
}
.friendlist td{
  padding-left:3px;
