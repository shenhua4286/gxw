							<ul class="swiper-slide ">
						{/if}
							<li class="{$index}">
								<a href="{$val['url']}&t=77&menu=1" onclick="parent.menu_activityIndex=2;parent.loadMenu(77);parent.menu(77);"
								target="main"
								>
									<img src="{$val['thumb']}" width="130" height="120" />
									<h5>{str_cut($val['title'],60,'...')}</h5>
									<p>{str_cut($val['description'],130,'...')}</p>
								</a>
							</li>
						{if $index%4==3}
							</ul>
						{/if}
						{php $index++;}
					{/loop}
				</div>
				<div class="swiper-pagination"></div>
			</div>
			<div class="arrowl"></div>
			<div class="arrowr"></div>
		</div>
