function BtnNormal(obj){
  obj.className="btn_normal";
}
function BtnOver(obj){
  obj.className="btn_hover";
}
function BtnDown(obj){
  obj.className="btn_down";
}
function toolNormal(obj){
  obj.className="toolbarout";
}
function toolOver(obj){
  obj.className="toolbarmove";
}
function toolDown(obj){
  obj.className="toolbardown";
}
function OnLoginKeyPress(ntype){
  switch(ntype){
    case "uid":
      //return event.returnValue=IsDigit();
      break;
    case "pwd":
      if(window.event.keyCode==13){
        document.all.loginform.submit();
      }
      break;
  }
}
function IsDigit(){
  return (((event.keyCode >= 48) && (event.keyCode <= 57)));
}